<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_card_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.onlinecamera.activity.CardDetailsActivity"
    android:background="@color/colorGreyDark">


    <include
        layout="@layout/toolbar_actionbar"
        android:id="@+id/toolbal_actionbar" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/contentCamera"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_below="@+id/toolbal_actionbar"
        android:layout_marginTop="8dp" />

</RelativeLayout>
